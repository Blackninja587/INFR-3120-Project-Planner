
<!-- Main Content -->
<main>
    <div class="container">
        <div class="row">
            <div class="col-md-offset-3 col-md-6">
                <h1><%= title %></h1>
                <br>
                <table class="table table-bordered table-striped table-hover">
                    <tr>
                        <th class="text-center"> Professor </th>
                        <th class="text-center"> Class </th>
                        <th class="text-center"> Assignment </th>
                        <th class="text-center"> Due_Date </th>
                        <th class="text-center"> Description </th>
                    </tr>
                    <!-- Repeat the template-->
                    <% for(let count=0; count < Assignments.length; count++) { %>
                        <tr>
                            <td class="text-center"><% Assignments[count].professor %></td>
                            <td class="text-center"><% Assignments[count].class %></td>
                            <td class="text-center"><% Assignments[count].assignment %></td>
                            <td class="text-center"><% Assignments[count].due_date %></td>
                            <td class="text-center"><% Assignments[count].description %></td>
                        </tr>
                    <% } %>
                </table>
            </div>
        </div>
    </div>
</main>